{
  "expo": {
    "name": "SleepyTV",
    "slug": "SleepyTV",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.chadrob.SleepyTV"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "extra": {
      "eas": {
        "projectId": "c7b3ff8c-61ef-4855-92ae-ee2e9c2ae1bc"
      }
    }
  },
  "plugins": [
    [
      "@react-native-tvos/config-tv",
      {
        "showVerboseWarnings": true
      }
    ]
  ]
}
